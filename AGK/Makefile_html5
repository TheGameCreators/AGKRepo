	mkdir -p $(OUT)/obj/freetype/src/cache
	mkdir -p $(OUT)/obj/freetype/src/gzip
	mkdir -p $(OUT)/obj/freetype/src/lzw
	mkdir -p $(OUT)/obj/freetype/src/smooth
	mkdir -p $(OUT)/obj/freetype/src/pcf
